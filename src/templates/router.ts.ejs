const internalHandlerWrapper = require('./internalHandlerWrapper').default;
<% methods.forEach((method) => {%>
import method<%-method%> from './<%-method%>';<% });%>

const errorMessages: any = <%- JSON.stringify(errorMessages || {}) %>;
const errorStatuses: any = <%- JSON.stringify(errorStatuses || {}) %>;

const methods: any = {
<% methods.forEach((method) => {%>    <%- method %>: internalHandlerWrapper(errorMessages, errorStatuses, method<%-method%>),<% });%>
};

export const handler = async (event: any, ctx: any) => {
    const { httpMethod } = event;

    return methods[httpMethod](event, ctx);
};
