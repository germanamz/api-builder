{
  "name": "@the-api-builder/builder",
  "version": "1.0.0",
  "description": "Build and develop serverless REST API's on your local machine",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/es/index.js",
  "keywords": [
    "serverless",
    "develop",
    "build",
    "rest",
    "api",
    "local"
  ],
  "author": "German Meza <iam@germanamz.com>",
  "homepage": "https://github.com/germanamz/api-builder#readme",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/germanamz/api-builder.git"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.json",
    "build:es": "tsc -p tsconfig.es.json",
    "clean": "rm -rf dist",
    "build": "yarn run clean && yarn run build:cjs && yarn run build:es"
  },
  "bugs": {
    "url": "https://github.com/germanamz/api-builder/issues"
  },
  "dependencies": {
    "@the-api-builder/errno": "^1.0.0",
    "@the-api-builder/errors": "^1.0.0",
    "@the-api-builder/pipeline-build": "^1.0.0",
    "@the-api-builder/pipeline-common": "^1.0.0",
    "@the-api-builder/pipeline-dev": "^1.0.0",
    "@the-api-builder/registry": "^1.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "typescript": "^4.3.5"
  }
}
