{
  "name": "@the-api-builder/builder",
  "version": "0.5.1",
  "description": "Build and develop serverless REST API's on your local machine",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/es/index.js",
  "keywords": [
    "serverless",
    "develop",
    "build",
    "rest",
    "api",
    "local"
  ],
  "author": "German Meza <iam@germanamz.com>",
  "homepage": "https://github.com/germanamz/api-builder#readme",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE.txt"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/germanamz/api-builder.git"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.json",
    "build:es": "tsc -p tsconfig.es.json",
    "clean": "rm -rf dist",
    "build": "yarn run clean && yarn run build:cjs && yarn run build:es"
  },
  "bugs": {
    "url": "https://github.com/germanamz/api-builder/issues"
  },
  "dependencies": {
    "@the-api-builder/errno": "^0.1.2",
    "@the-api-builder/errors": "^0.1.3",
    "@the-api-builder/pipeline-build": "^0.5.1",
    "@the-api-builder/pipeline-common": "^0.5.1",
    "@the-api-builder/pipeline-dev": "^0.5.1",
    "@the-api-builder/registry": "^0.2.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "typescript": "^4.3.5"
  },
  "gitHead": "0a348e1f25d1613e9f06e8f7a19ada88fa2f63ad"
}
